{
  "name": "mrya",
  "displayName": "Mrya",
  "description": "Syntax highlighting and snippets for the Mrya language",
  "version": "0.6.1",
  "publisher": "gaupestudio",
  "engines": {
    "vscode": "^1.104.0"
  },
  "activationEvents": [
    "onLanguage:mrya"
  ],
  "main": "./src/extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mrya",
        "aliases": [
          "Mrya",
          "mrya"
        ],
        "extensions": [
          ".mrya"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mrya",
        "scopeName": "source.mrya",
        "path": "./syntaxes/mrya.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mrya",
        "path": "./snippets/mrya.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0"
  }
}
